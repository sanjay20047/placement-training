// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
bool search(char c[],char target,int n){
    for(int i=0;i<n;i++){
        if(c[i]==target){
            return true;
        }
    }
    return false;
}
int main(){
    char c[]="laptop";
    char target='e';
    int n=sizeof(c)/sizeof(c[0]);
    if(search(c,target,n)){
        cout<<"yes";
        
    }
    else{
        cout<<"no";
    }
}
