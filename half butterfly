import java.util.*;
class main{
    public static void main(String args[]){
        int row=10;
        int col=4;
        for(int i=1;i<=row;i++){
             for(int h=1;h<=i;h++){
                 System.out.print("*");
             }
            for(int k=1;k<=2*(row)-2*i;k++){
                System.out.print(" ");
            }
            for(int j=1;j<=i;j++){
                System.out.print("*");
                
            }
            System.out.println(" ");
        }
    }
}
